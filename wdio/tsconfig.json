{
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "types": [
      "node",
      "@wdio/globals/types",
      "@wdio/cucumber-framework",
      "webdriverio/async"
    ]
  },
  "include": ["src"]
}
